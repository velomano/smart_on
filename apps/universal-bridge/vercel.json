{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci",
  "regions": ["icn1"],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "SUPABASE_URL": "https://placeholder.supabase.co",
      "SUPABASE_SERVICE_ROLE_KEY": "placeholder-key",
      "BRIDGE_ENCRYPTION_KEY": "placeholder-encryption-key",
      "BRIDGE_HTTP_PORT": "3000",
      "BRIDGE_WS_PORT": "8080",
      "BRIDGE_SERVER_URL": "https://universal-bridge.vercel.app",
      "WEB_ADMIN_URL": "https://smart-on-web-admin.vercel.app",
      "LOG_LEVEL": "info",
      "SIGNATURE_VERIFY_OFF": "true"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
